    "msw": "^1.3.2",
    "prettier": "^2.8.8",
    "react-scripts": "^5.0.1",
    "react-select-event": "^5.5.1",
    "react-test-renderer": "^18.2.0",
    "source-map-explorer": "^2.5.3"
  },
