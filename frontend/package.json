    "msw": "0.49.2",
    "prettier": "^2.8.8",
    "react-select-event": "^5.5.1",
    "react-test-renderer": "^17.0.2",
    "source-map-explorer": "^2.5.3"
  },
  "jest": {
    "coverageReporters": [
      "html"
    ],
    "transformIgnorePatterns": [
      "node_modules/@uiw/react-md-editor/"
    ]
  },
  "volta": {
